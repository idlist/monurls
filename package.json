    "fastify": "^3.29.0",
    "fastify-plugin": "^3.0.1",
    "fastify-schedule": "^1.0.0",
    "js-yaml": "^4.1.0",
    "luxon": "^2.4.0",
    "mariadb": "^3.0.0",
    "normalize.css": "^8.0.1",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "toad-scheduler": "^1.6.0"
  },
  "devDependencies": {
    "@rollup/plugin-yaml": "^3.1.0",
    "@types/js-yaml": "^4.0.5",
