{
  "name": "monurls",
  "version": "0.0.1",
  "description": "My own Node.js URL shortener.",
  "author": "i'DLisT",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/idlist/monurls.git"
  },
  "bugs": {
    "url": "https://github.com/idlist/monurls/issues"
  },
  "homepage": "https://github.com/idlist/monurls#readme",
  "main": "backend/dist/index.js",
  "scripts": {
    "dev": "vite",
    "bundle": "vite build",
    "build": "vite build && node backend/scripts/build",
    "compile": "node backend/scripts/build",
    "start": "node backend/scripts/build && node backend/dist/index",
    "prod": "vite build && node backend/scripts/build && node backend/dist/index",
    "host": "node backend/dist/index",
    "count": "cd .. && cloc monurls --exclude-dir=node_modules,dist --include-ext=ts,tsx,sass"
  },
  "dependencies": {
    "@vitejs/plugin-react-refresh": "^1.0.0",
    "axios": "^0.21.0",
    "fastify": "^3.0.0",
    "fastify-cookie": "^5.0.0",
    "fastify-cors": "^6.0.0",
    "fastify-plugin": "^3.0.0",
    "fastify-rate-limit": "^5.0.0",
    "fastify-schedule": "^1.0.0",
    "fastify-static": "^4.0.0",
    "js-yaml": "^3.0.0",
    "luxon": "^2.0.0",
    "mariadb": "^2.0.0",
    "normalize.css": "^8.0.0",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "toad-scheduler": "^1.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-yaml": "^3.0.0",
    "@types/js-yaml": "^4.0.0",
