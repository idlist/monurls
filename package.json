    "chalk": "^5.0.1",
    "esbuild": "^0.14.39",
    "esbuild-node-externals": "^1.4.1",
    "eslint": "^8.16.0",
    "eslint-plugin-react": "^7.30.0",
    "glob": "^8.0.3",
    "sass": "^1.52.1",
    "vite": "^2.9.9"
  }
}
